{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "react": [ "./node_modules/@types/react"],
      "@/*": [
        "./*"
      ],
      "@/images/*": [
        "./public/assets/images/*"
      ],
      "@/components/*": [
        "./src/components/*"
      ],
      "@/hooks/*": [
        "./src/hooks/*"
      ],
      "@/pages/*": [
        "./src/pages/*"
      ],
      "@/styles/*": [
        "./src/styles/*"
      ],
      "@/utils/*": [
        "./src/utils/*"
      ]
    },
    "checkJs": true,
    "jsx": "react",
    "lib": ["es2017"],
    "strict": true,
  }
}